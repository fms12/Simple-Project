  <body >
    <form>
        <h1 id ='form-heading'>Register</h1>
      <div id='soical-wrapper'>
        <img class='soical-icon' src='http://pluspng.com/img-png/google-logo-png-open-2000.png' alt=''/>
         <img class='soical-icon' src='https://cdn4.iconfinder.com/data/icons/social-media-icons-the-circle-set/48/facebook_circle-512.png' alt=''/>
          <img class='soical-icon' src='https://image.flaticon.com/icons/svg/25/25231.svg' alt=''/>
